{
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always",
    "source.sortImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.configPath": ".prettierrc",
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "dpage",
    "healthcheck",
    "isready",
    "pgadmin",
    "pgdata",
    "timestamptz"
  ]
}
